<h1>Programs#index</h1>
<%= link_to "Add A Program", new_program_path %>
<% if @programs.empty? %>
    <h2>There are no programs at the moment. </h2>
<% else %> 
    <% @programs.each do |i| %>
        Name: <%= i.name %> <br>
        Description: <%= i.description %><br>
        Website: <%= i.website %><br>
    <% end %>
<% end %>
