<h2>Add A Program</h2>
<%= form_with model: @program, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <br>
    <%= f.label :description %>
    <%= f.text_area :description %><br>
    <br>
    <%= f.label :website %>
    <%= f.text_field :website %><br>
    <br>
    <%= f.submit 'Submit' %>
<% end %>